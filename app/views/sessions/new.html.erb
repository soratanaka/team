<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>
<% end %>

これ削除
<%if current_user.present? %>
  <li><%= link_to "ログアウト", session_path(current_user.id), method: :delete %></li>
<% end %>